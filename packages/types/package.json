{
  "name": "@breeze/types",
  "private": true,
  "version": "0.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*",
      "default": "./src/*"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@horizon/eslint-config": "*",
    "@horizon/typescript-config": "*",
    "@breeze/icons": "*"
  }
}
